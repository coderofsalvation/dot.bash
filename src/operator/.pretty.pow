.pretty()
  local str=$(cat)
  # check whether json
  local first=${str:0:1}
  local last=${str:$((${#str}-1))}
  if $first == "{" || $first == "["       
    if $last == "}" || $last == "]"       
      if which python2 &>/dev/null
        echo "$str" | python -mjson.tool
        return 0
      if which node &>/dev/null
        node -e "console.log(JSON.stringify(JSON.parse(process.argv[1]), null, 2));" "$str"
        return 0
      #echo "$str" | json_pp
  echo "$str"
